  "text": [
    "a small nativity,",
    "mother, baby, sheep",
    "made of white",
    "and blue balloons.",
    "*",
    "Sky",
    "god",
    "girl.",
    "Pick out the one",
    "that doesn\u2019t belong.",
    "Some thing",
    "close to nothing",
    "flat",
    "from which,",
    "fatherless,",
    "everything has come."
  ],
