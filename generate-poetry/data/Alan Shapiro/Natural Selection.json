  "text": [
    "proceeds by chance",
    "and necessity",
    "becomes nonrandom",
    "through randomness",
    "builds complexity",
    "from simplicity",
    "nurtures consciousness",
    "unconsciously",
    "evolves purposelessly",
    "creatures who demand",
    "purpose",
    "and discover",
    "natural selection"
  ],
  "title": "Natural Selection",
  "year": ""
}