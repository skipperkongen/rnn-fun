  "text": [
    "the whole moon",
    "burns behind jamestown",
    "seven wings of geese",
    "light the thin ice",
    "the asian sun",
    "bloody on the interstate",
    "spring flowers",
    "break on the gray prairie",
    "fingerprints",
    "on the rearview mirror",
    "feral shadows",
    "transposed near fargo"
  ],
