  "text": [
    "In prayer:",
    "quiet opening,",
    "my artery is a thin",
    "shadow on paper\u2014",
    "margin of long grass,",
    "ruderal hair, sister to this",
    "not yet part of our bodies",
    "your lyric corpus of seed",
    "in rough drafts of pine ash,",
    "or grass calligraphy",
    "in rough drafts of pine ash\u2014",
    "your lyric corpus of seed",
    "not yet part of our bodies:",
    "ruderal hair, sister to this",
    "margin of long grass,",
    "shadow on paper,",
    "my artery is a thin",
    "quiet opening",
    "in prayer."
  ],
  "title": "Dream of Ink Brush Calligraphy",
  "year": ""
}