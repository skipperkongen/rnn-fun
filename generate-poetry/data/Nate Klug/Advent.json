  "text": [
    "to start over",
    "to assume again",
    "an order",
    "at the end",
    "of wonder",
    "to conjure",
    "and then to keep",
    "slow dirty sleet",
    "within its streetlight"
  ],
  "title": "Advent",
  "year": ""
}