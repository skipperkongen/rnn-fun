  "text": [
    "Boughs berserk",
    "Spin one hill",
    "Into space",
    "Standing still",
    "Olive trees race",
    "On the field below",
    "Moulded white oxen",
    "Ponder each furrow",
    "A man behind them",
    "Cries"
  ],
  "title": "Landscape",
  "year": ""
